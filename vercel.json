{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/workouts",
      "dest": "/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/api/v1/docs",
      "destination": "/v1/swagger-ui/index.html?url=/v1/docs.json"
    }
  ] 
}
